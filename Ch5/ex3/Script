docker-compose up -d
docker-compose up -d

docker-compose -p new up -d

docker-compose down
docker-compose -p new down

docker-compose up -d

docker-compose down

docker-compose -f docker-compose.yml -f docker-compose-new.yml config

docker-compose -f docker-compose.yml -f docker-compose-new.yml up -d

docker-compose -f docker-compose.yml -f docker-compose-new.yml down
